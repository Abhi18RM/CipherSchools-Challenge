<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CipherSchools</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
        integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <link rel="stylesheet" href="css/styles.css">
    <link rel="icon" href="/images/Cipherschools_icon@2x.3b571d743ffedc84d039.png">
</head>

<body>
    <div class="p-3 mb-3  header">
        <div class="container">
            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <div class="logo_details">
                    <img class="bx bxl-audible icon" src="/images/Cipherschools_icon@2x.3b571d743ffedc84d039.png"></img>
                    <div class="logo_name">CipherSchools</div>
                </div>
                <div class="dropdown text-end">
                    <a href="#" class="d-block link-dark text-decoration-none dropdown-toggle drop"
                        data-bs-toggle="dropdown" aria-expanded="false">
                        <i class="bx bx-compass"></i>
                        <span style="font-size: 14px;
                        padding: 3px;">Browse</span>
                    </a>
                    <ul class="dropdown-menu text-small">
                        <li><a class="dropdown-item" href="#">App Development</a></li>
                        <li><a class="dropdown-item" href="#">Web Development</a></li>
                        <li><a class="dropdown-item" href="#">Game Development</a></li>
                        <li><a class="dropdown-item" href="#">Data Structures</a></li>
                        <li><a class="dropdown-item" href="#">Programming</a></li>
                        <li><a class="dropdown-item" href="#">Machine Learning</a></li>
                        <li><a class="dropdown-item" href="#">Data Science</a></li>
                        <li><a class="dropdown-item" href="#">Others</a></li>
                    </ul>
                </div>

                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">

                </ul>

                <form class="col-12 col-lg-auto mb-4 mb-lg-0 me-lg-3" role="search">
                    <input type="search" class="form-control" placeholder="Search and learn.." aria-label="Search">
                </form>

                <div class="col-auto col-lg-auto mb-lg-0 me-lg-3">
                    <i class="bx bx-bell nav-icon"></i>
                </div>

                <div class="dropdown text-end">
                    <a class="d-block link-dark text-decoration-none dropdown-toggle" data-bs-toggle="dropdown"
                        aria-expanded="false">
                        <img src="https://media.istockphoto.com/id/1300845620/vector/user-icon-flat-isolated-on-white-background-user-symbol-vector-illustration.jpg?s=612x612&w=0&k=20&c=yBeyba0hUkh14_jgv1OKqIH0CCSWU_4ckRkAoy2p73o="
                            alt="mdo" width="32" height="32" class="rounded-circle">
                    </a>
                    <ul class="dropdown-menu text-small">
                        <li><a class="dropdown-item" href="#">Dashboard</a></li>
                        <li><a class="dropdown-item" href="#">Profile</a></li>
                        <li><a class="dropdown-item" href="#">Enrolled Courses</a></li>
                        <li><a class="dropdown-item" href="#">Wishlist</a></li>
                        <li><a class="dropdown-item" href="#">Liked Videos</a></li>
                    </ul>
                </div>

                <div class="col-auto col-lg-auto mb-lg-0 me-lg-3">
                    <i class="bx bx-copyright nav-icon"></i>
                </div>

                <div class="col-auto col-lg-auto mb-lg-0 me-lg-3">
                    <i class="bx bx-moon nav-icon"></i>
                </div>

            </div>
        </div>
    </div>
    <div class="sidebar">
        <div class="logo_details">
            <img class="bx bxl-audible icon" src="/images/Cipherschools_icon@2x.3b571d743ffedc84d039.png"></img>
            <div class="logo_name">CipherSchools</div>
            <i class="bx bx-menu" id="btn"></i>
        </div>
        <ul class="nav-list">
            <li>
                <a href="#">
                    <i class="bx bx-home"></i>
                    <span class="link_name">Home</span>
                </a>
                <span class="tooltip">Home</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bx-book-reader"></i>
                    <span class="link_name">Courses</span>
                </a>
                <span class="tooltip">Courses</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bx-compass"></i>
                    <span class="link_name">Trending</span>
                </a>
                <span class="tooltip">Trending</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bx-user-check"></i>
                    <span class="link_name">Following</span>
                </a>
                <span class="tooltip">Following</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bxs-dashboard"></i>
                    <span class="link_name">Dashboard</span>
                </a>
                <span class="tooltip">Dashboard</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bxl-discord-alt"></i>
                    <span class="link_name">Discord</span>
                </a>
                <span class="tooltip">Discord</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bx-message-alt-edit"></i>
                    <span class="link_name">Registration Form</span>
                </a>
                <span class="tooltip">Registration Form</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bx-copyright"></i>
                    <span class="link_name">Copyright</span>
                </a>
                <span class="tooltip">Copyright</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bxs-message-detail" style="color: #000000"></i>
                    <span class="link_name">Feedback</span>
                </a>
                <span class="tooltip">Feedback</span>
            </li>
            <li>
                <a href="#">
                    <i class="bx bxs-directions"></i>
                    <span class="link_name">Directions</span>
                </a>
                <span class="tooltip">Directions</span>
            </li>
            <li class="profile">
                <div class="profile_details">
                    <img src="https://media.istockphoto.com/id/1300845620/vector/user-icon-flat-isolated-on-white-background-user-symbol-vector-illustration.jpg?s=612x612&w=0&k=20&c=yBeyba0hUkh14_jgv1OKqIH0CCSWU_4ckRkAoy2p73o="
                        alt="profile image">
                    <div class="profile_content">
                        <div class="name">Anna Jhon</div>
                        <div class="designation">Admin</div>
                    </div>
                </div>
                <form action="/logout" method="POST">
                    <button type="submit">
                        <i class="bx bx-log-in-circle" id="log_out"></i>
                    </button>
                </form>
            </li>
        </ul>
    </div>

    <div class="home-section">
        <div class="pf-user-box">
            <div class="pf-user-back">
                <div class="pf-user-content">
                    <div class="pf-user-profile-box">
                        <div class="MuiAvatar-root MuiAvatar-circular pf-user-profile"><img alt="user-profile"
                                src="https://media.istockphoto.com/id/1300845620/vector/user-icon-flat-isolated-on-white-background-user-symbol-vector-illustration.jpg?s=612x612&w=0&k=20&c=yBeyba0hUkh14_jgv1OKqIH0CCSWU_4ckRkAoy2p73o="
                                class="MuiAvatar-img"></div>

                    </div>
                    <div class="pf-user-details">
                        <div class="pf-user-detail-box">
                            <div id="pf-user-side">
                                <h2 class="pf-user-greeting">Hello,</h2>
                                <h1 class="pf-user-name" title="Abhiram Ravuri">
                                    <%= userName %>
                                </h1>
                                <h2 class="pf-user-email">
                                    <%= userEmail %>>
                                </h2>
                            </div>
                            <div class="pf-right-side">
                                <div class="pf-user-followers-count"><span>0</span> Followers</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>